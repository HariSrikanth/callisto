    "postcss": "^8.4.35",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "shadcn-ui": "^0.8.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "@types/express": "^4.17.21",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
