    "react-resizable-panels": "^2.0.13",
    "react-router-dom": "^7.5.1",
    "sonner": "^1.4.3",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.0",
    "zod": "^3.22.4",
    "@anthropic-ai/sdk": "^0.18.0",
    "@modelcontextprotocol/sdk": "^1.10.2",
